{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Ocelot.Extensions": {
    "Configuration": {
      "Enabled": true,
      "RepositoryType": "AzureStorage",
      "CheckingInterval": 10,
      "AzureStorage": {
        "Type": "Blob",
        "AccessType": "SharedKey",
        "AccountName": "ocelotextensions",
        "AccountKey": "[removed]",
        "ResourceUri": "clouddddr/ocelot.json"
      }
    }
  }
}
